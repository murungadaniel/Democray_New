                        <cell halign="center"></cell>
                    </row>
                </tabular>
                </statement>
                
                <answer>
                    <p>
                       <m>
                            \begin{align}
                            \text{F} &amp;= 1000 \left( 1 + \dfrac{0.05}{1} \right)^{1 \times 3}\\
                            &amp;= \$1{,}157.63
                            \end{align}
                        </m> 
                    </p>
                </answer>
            </activity>

